{
  "manifest_version": 2,

  "name": "One-Click BibTex",
  "description": "This extension gets your bibtex citation through url within one-click. ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/", 
	"https://scholar.google.com/"
  ]
}
