{
  "manifest_version": 2,

  "name": "BibTex It!",
  "description": "This extension gets your bibtex citation through your text selection within one-click. ",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "https://scholar.google.com/"
  ],
  "background": {
    "scripts": ["js/context.js"]
  }
}
